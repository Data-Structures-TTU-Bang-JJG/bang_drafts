int death (player_lineup *q){
	if (var_player.bullets == 0)
		if (q->player_lineup.front == q->player_lineup.rear){
			player_lineup.data = q->play_lineup.front->dat;
			free(q-> play_lineup.front)
			q->player_lineup.front=NULL;
			q->player_lineup.rear=NULL;
		}
		else{
			struct temp node *temp = q->player_lineup.front;
			q->player_lineup.front=q->player_line.front->link;
			q->player_lineup.rear->link=q->player_lineup.front;
			free (temp);
		}
	return player_lineup.data
}

struct endgame (player_lineup *){
	if(var_player.role=1 != player_lineup.front){
		cout>>"The outlaws win!";
	}
	if(var_player.role=3 = player_lineup.front && player_lineup.rear){
		cout>>"The renegades win!";
	}
	if(var_player.role=1 )
}		

